
public class Test {

	public static void main(String[] args) {
		
		//변수 값의 합을 구하는 프로그램
		int [][] a = {{10,20,50},{20,30,15},{100,110,120}};
		
		int sum = 0;
		for (int i=0;i<a.length;i++) { //첫번째 배열의 
			for (int j=0;j<a.length;j++) {
				sum+=a[i][j];
			}
		}
		System.out.print("변수의 합은 > " +sum);
	}

}
