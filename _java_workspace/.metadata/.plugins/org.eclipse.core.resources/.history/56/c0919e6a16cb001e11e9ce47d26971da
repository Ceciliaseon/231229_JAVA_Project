
public class Card2 {
	
	private int num;
	private char shape;
	
	public Card2 () {
		this.num = 1;
		this.shape = 'â—†';
	}
	
	public void print () {
		System.out.println(shape);
		
		if (num == 11) {
			System.out.println("J"+" ");
		} else if (num == 12) {
			System.out.println("Q"+" ");
		} else if (num == 13) {
			System.out.println("K"+" ");
		} else {
			System.out.println(num+" ");
		}
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public char getShape() {
		return shape;
	}

	public void setShape(char shape) {
		this.shape = shape;
	}
	
	
}
