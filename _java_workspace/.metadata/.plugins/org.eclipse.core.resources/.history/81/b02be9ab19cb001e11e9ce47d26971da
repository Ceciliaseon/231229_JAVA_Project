
public class CardpackMain {

	public static void main(String[] args) {
		
		CardPack2 pack2 = new CardPack2();
		pack2.random();
		
		int cnt = 0;
		for (int i=0;i<4;i++) { // shape
			for (int j=0;j<13;j++) { // num
				pack2.getCardpack2()[cnt].print();
				cnt++;
			}
			System.out.println();
		}
		System.out.println();
		
		pack2.put().print();
		pack2.put().print();
		pack2.put().print();
		pack2.put().print();
		pack2.put().print();
	}

}
