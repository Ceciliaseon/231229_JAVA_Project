
class CardPack2 {


	private Card2 [] Cardpack2 = new Card2 [52];
	private int a;

	public CardPack2 () {
		
		char shape = '◆';
		for (int i=1;i<=4;i++) {
			switch (shape) {
			case '♥':
				break;
			case '♣':
				break;
			case '◆':
				break;
			case '♠':
				break;

			default:
				break;
			}
		}
	}


	public static void main(String[] args) { // 확인

	}

}